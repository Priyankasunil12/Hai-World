package polymorphism;

 class ADD {
	
 public int ADD(int a,int b)
	{
		return (a+b);
	}
	public int ADD(int a,int b,int c) 
		{
		return (a+b+c);	
		}
public double ADD(double a,double b)
{
	return (a+b);
}
	public static void main(String[] args) {
	ADD poly=new ADD();
	System.out.println(poly.ADD(2,3));
	System.out.println(poly.ADD(4,5,6));
	System.out.println(poly.ADD(2.4,3.5));
	

	}

}
