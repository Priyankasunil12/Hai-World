
public class Inheritance {
	String name="Inheritance;"
	public void display()
	{
		System.out.println("Its a super class");
		
	}
}
class subclass extends Inheritance
{
	public void execute()
	{
		System.out.println("Its a sub class");
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
