
public class BasicMethod {
	public static void getminimum(int a ,int b)
	{
		if(a>b)
		{
			System.out.println("a is greater");
		}
		else if(b>a)
		{
			System.out.println("b is greater");
		}
		else
			System.out.println("equal");
	}

	public static void main(String[] args) {
		getminimum(500,1000);
	

	}

}
